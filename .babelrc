    "presets": ["next/babel"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["."],
          "alias": {
            "@pages": "./src/app/pages",
            "@components": "./src/app/components"
          },
          "extensions": [".js", ".jsx", ".tsx"]
        }
      ]
    ]
  }
